<div class="container top bottom info">
  <h2>Notes</h2>
  <br />
    <% current_user.notes.each do |note| %>
    <p class="info">
      <%= note.message %><br />
    </p>
    <% end %>
</div>
<div class="container top">
  <h2>Trends</h2>
  <p class="info" style="color: firebrick;">
    <%= current_user.users_duplicate_notes %>
  </p>
</div>
